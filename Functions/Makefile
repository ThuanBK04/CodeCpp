.PHONY: clean All

All:
	@echo "----------Building project:[ RecursiveFunctions - Debug ]----------"
	@cd "RecursiveFunctions" && "$(MAKE)" -f  "RecursiveFunctions.mk"
clean:
	@echo "----------Cleaning project:[ RecursiveFunctions - Debug ]----------"
	@cd "RecursiveFunctions" && "$(MAKE)" -f  "RecursiveFunctions.mk" clean
